{
  "name": "aoj-helper",
  "displayName": "AOJ Helper",
  "description": "",
  "version": "0.0.3",
  "publisher": "s1280148",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Education"
  ],
  "preview": true,
  "activationEvents": [
    "onView:AOJ.AOJView"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "AOJ",
          "title": "AOJ",
          "icon": "./static/images/AOJ.png"
        }
      ]
    },
    "views": {
      "AOJ": [
        {
          "type": "webview",
          "id": "AOJ.AOJView",
          "name": "Aizu Online Judge"
        }
      ]
    },
    "commands": [
      {
        "command": "AOJ.submit",
        "title": "AOJ: Submit"
      }
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.12",
    "@types/react-copy-to-clipboard": "^5.0.6",
    "@vscode/test-electron": "^2.3.0",
    "@vscode/vsce": "^2.19.0",
    "async-mutex": "^0.4.0",
    "axios": "^1.3.6",
    "better-react-mathjax": "^2.0.2",
    "glob": "^8.0.3",
    "i18next": "^23.7.7",
    "mocha": "^10.0.0",
    "monaco-editor": "^0.44.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.4.3",
    "tailwindcss": "^3.3.1",
    "web-vitals": "^2.1.4",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/babel__core": "^7.20.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@types/vscode": "^1.77.0",
    "@types/vscode-webview": "^1.57.1",
    "@types/webpack": "^5.28.1",
    "@types/webpack-dev-server": "^4.7.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "autoprefixer": "^10.4.14",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.39.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.3",
    "prettier": "3.0.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.3.1",
    "typescript": "^4.9.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "build production": "webpack --config ./webpack.config.webview-main.js --mode production && webpack --config ./webpack.config.extension.js --mode production",
    "build development": "webpack --config ./webpack.config.webview-main.js --mode=development --devtool=source-map && webpack --config ./webpack.config.extension.js --mode=development --devtool=source-map"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
